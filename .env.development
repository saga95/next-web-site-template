# Development Environment
# Auto-loaded when NODE_ENV=development

# Application
NEXT_PUBLIC_APP_NAME=My Next Template (Dev)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# API
NEXT_PUBLIC_API_URL=http://localhost:3001

# EmailJS (use test credentials)
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_dev_service_id
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your_dev_template_id
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_dev_public_key

# Analytics (disabled in dev)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=false

# Feature Flags
NEXT_PUBLIC_ENABLE_PWA=false
NEXT_PUBLIC_ENABLE_I18N=true

# Environment
NODE_ENV=development
